<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=System.Runtime">

    <SolidColorBrush x:Key="Window.Static.Background" Color="#151515"/>
    <SolidColorBrush x:Key="Panel.Static.Background" Color="#1F1F1F"/>
    <SolidColorBrush x:Key="Border.Static.Brush" Color="Transparent"/>
    <SolidColorBrush x:Key="TextBlock.Static.Foreground" Color="WhiteSmoke"/>
    <SolidColorBrush x:Key="TextBlock.Static.SecondFG" Color="#dbdbdb"/>
    <SolidColorBrush x:Key="TextBlock.Static.Background" Color="#383838"/>
    <SolidColorBrush x:Key="Notifi.Background" Color="#383838"/>

    <sys:String x:Key="Notifi.Border.Brush.Normal">#383838</sys:String>
    <sys:String x:Key="Notifi.Border.Brush.Fault">#db3939</sys:String>
    <sys:String x:Key="Notifi.Border.Brush.Warning">#e3b65d</sys:String>

</ResourceDictionary>