<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=System.Runtime">

    <SolidColorBrush x:Key="Window.Static.Background" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="Panel.Static.Background" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="Border.Static.Brush" Color="#d4d4d4"/>
    <SolidColorBrush x:Key="TextBlock.Static.Foreground" Color="Black"/>
    <SolidColorBrush x:Key="TextBlock.Static.SecondFG" Color="#2e2e2e"/>
    <SolidColorBrush x:Key="TextBlock.Static.Background" Color="#E6E6E6"/>
    <SolidColorBrush x:Key="Notifi.Background" Color="#fafafa"/>

    <sys:String x:Key="Notifi.Border.Brush.Normal">#d4d4d4</sys:String>
    <sys:String x:Key="Notifi.Border.Brush.Fault">#db3939</sys:String>
    <sys:String x:Key="Notifi.Border.Brush.Warning">#e3b65d</sys:String>

</ResourceDictionary>